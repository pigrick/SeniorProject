<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>servlet.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateBlogEntryServlet</servlet-name>
        <servlet-class>servlet.CreateBlogEntryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BlogEntryServlet</servlet-name>
        <servlet-class>servlet.BlogEntryListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>servlet.registerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editBlogEntryServlet</servlet-name>
        <servlet-class>servlet.editBlogEntryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteBlogEntryServlet</servlet-name>
        <servlet-class>servlet.deleteBlogEntryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewBlogEntryServlet</servlet-name>
        <servlet-class>servlet.viewBlogEntryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>browseServlet</servlet-name>
        <servlet-class>servlet.browseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteCommentServlet</servlet-name>
        <servlet-class>servlet.deleteCommentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editProfileServlet</servlet-name>
        <servlet-class>servlet.editProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>likecommentServlet</servlet-name>
        <servlet-class>servlet.likecommentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>likeblogentryServlet</servlet-name>
        <servlet-class>servlet.likeblogentryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateBlogEntryServlet</servlet-name>
        <url-pattern>/CreateBlogEntry</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BlogEntryServlet</servlet-name>
        <url-pattern>/BlogEntryList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editBlogEntryServlet</servlet-name>
        <url-pattern>/BlogEntryList/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteBlogEntryServlet</servlet-name>
        <url-pattern>/BlogEntryList/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewBlogEntryServlet</servlet-name>
        <url-pattern>/BlogEntryList/view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>browseServlet</servlet-name>
        <url-pattern>/browse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteCommentServlet</servlet-name>
        <url-pattern>/BlogEntryList/comment/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editProfileServlet</servlet-name>
        <url-pattern>/editProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>likecommentServlet</servlet-name>
        <url-pattern>/BlogEntryList/likecomment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>likeblogentryServlet</servlet-name>
        <url-pattern>/BlogEntryList/likeblogentry</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>MainPage.jsp</welcome-file>
    </welcome-file-list>
</web-app>
